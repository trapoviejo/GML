#ifndef GML_TIPOS
#define GML_TIPOS

enum gmltipos {
     TIPO_INT = 10000,
     TIPO_FLOAT = 10001,
     TIPO_POS = 10002,
     TIPO_STRING = 10003,
     TIPO_BOOLEAN = 10004,
     TIPO_ENTITY = 10005,
     TIPO_LIST = 10006,
     TIPO_POINTER = 19997,
     TIPO_VOID = 19998,
     TIPO_PROGRAMA = 19999
   };
 #endif
 
 /* Tokens.  */
#define TIPO_INT 10000
#define TIPO_FLOAT 10001
#define TIPO_POS 10002
#define TIPO_STRING 10003
#define TIPO_BOOLEAN 10004
#define TIPO_ENTITY 10005
#define TIPO_LIST 10006
#define TIPO_VOID 19998
#define TIPO_PROGRAMA 19999

 /* Tokens.  */
#define GML_ES_VARIABLE 30000
#define GML_ES_TEMPORAL 30001
#define GML_ES_CONSTANTE 30002
