program mijuego;
vars int: vidas, a, b;
	 float: puntos, total;
	 boolean: prueba;

mapsize [256,78];

void algo(int x, float y){
    puntos = x*y;
}


{
    algo(a+b, b-a);
    total = puntos * vidas + puntos;
    if(total>vidas && prueba){
	    vidas = vidas * 2;
	}
	total = total + total * vidas;    

}
